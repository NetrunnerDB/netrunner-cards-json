{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": false,
    "properties": {
        "code": {
            "minLength": 1,
            "type": "string"
        },
        "date_start": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ]
        },
        "name": {
            "minLength": 1,
            "type": "string"
        },
        "rotated": {
            "items": {
                "type": "string"
            },
            "type": "array"
        }
    },
    "required": [
        "code",
        "date_start",
        "name",
        "rotated"
    ],
    "type": "object"
}
